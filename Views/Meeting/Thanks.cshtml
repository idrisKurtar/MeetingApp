@model UserInfo
@{
    ViewBag.Title = "Başvuru Durumu";
}
<main class="text-center">
<h1 class="h4">Başvuru Durumu</h1>
@if(Model.WillAttend == true)
 {
    <p>Teşekkürler @Model.Name, toplantıya sizinle birlikte @ViewBag.UserCount katılıyor</p>
   <a class="btn btnsm btn-outline-primary" href="/meeting/list">Katılımcıları Listele</a>
}
else
{
    <p>@Model.Name katılamadığınız için üzügünüz</p>
    <a class="btn btnsm btn-outline-primary" href="/Meeting/apply">Başvuru yap</a>


}



</main>